<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="clm.developers.login"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="clm.developers.login.MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="clm.developers.login.PlaceActivity"
            android:label="@string/title_activity_place" >
        </activity>
        <activity
            android:name="clm.developers.login.MenuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="clm.developers.login.MenuVentaCobranzaActivity"
            android:label="@string/title_activity_menu_venta_cobranza" >
        </activity>
        <activity
            android:name="clm.developers.login.ProductosListaActivity"
            android:label="@string/title_activity_productos_lista" >
        </activity>
        <activity
            android:name="clm.developers.login.AgregarVentaContadoActivity"
            android:label="@string/title_activity_agregar_venta_contado" >
        </activity>
        <activity
            android:name="clm.developers.login.RegistrarVentaActivity"
            android:label="@string/title_activity_registrar_venta" >
        </activity>
        <activity
            android:name="clm.developers.login.LugarActivity"
            android:label="@string/title_activity_lugar" >
        </activity>
        <activity
            android:name="clm.developers.cobranza.ClienteActivity"
            android:label="@string/title_activity_cliente" >
        </activity>
        <activity
            android:name="clm.developers.cobranza.CuentaActivity"
            android:label="@string/title_activity_cuenta" >
        </activity>
        <activity
            android:name="clm.developers.cobranza.ClientepagoActivity"
            android:label="@string/title_activity_clientepago" >
        </activity>

        <meta-data
            android:name="android.app.default_searchable"
            android:value="clm.developers.library.SearchActivity" />

        <activity
            android:name="clm.developers.library.SearchActivity"
            android:label="@string/title_activity_search" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>

        <activity
            android:name="clm.developers.cobranza.NuevocActivity"
            android:label="@string/title_activity_nuevoc" >
        </activity>
    </application>

</manifest>